		"deep-equal": "^2.0.4",
		"discord-anti-spam": "^2.4.1",
		"discord-oauth2": "^2.2.0",
		"discord.js": "^12.3.0",
		"dom-parser": "^0.1.6",
		"dompurify": "^2.0.17",
